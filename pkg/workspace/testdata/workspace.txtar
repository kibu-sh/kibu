mkdir -p $WORK/level1/level2/level3
determine $WORK/level1/level2/level3
load $WORK/level1/level2/level3

-- .kibu/workspace.cue --
package workspace

import (
  "discern.com/kibue"
)

kibue.#Workspace & {
  ConfigStore: {
    EncryptionKeys: [{
      Env: "development"
      Engine: "gcpkms"
      Key: "projects/PROJECT_ID/locations/global/keyRings/KEYRING_ID/cryptoKeys/KEY_ID"
    }]
  }
}